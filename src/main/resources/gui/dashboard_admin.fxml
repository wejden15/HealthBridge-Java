<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="gui.AdminDashboardController"
      spacing="20" padding="20"
      style="-fx-background-color: #ffffff;">

    <Label text="Admin Dashboard - Doctor Approval"
           style="-fx-font-size: 20px; -fx-font-weight: bold;" />

    <TableView fx:id="doctorTable" prefHeight="300">
        <columns>
            <TableColumn text="ID" prefWidth="50" />
            <TableColumn text="Name" prefWidth="150" />
            <TableColumn text="Email" prefWidth="200" />
            <TableColumn text="Action" prefWidth="100" />
        </columns>
    </TableView>

    <Button text="Logout" style="-fx-pref-width: 100;" />
</VBox>

